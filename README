Mootools MeioMask plugin for Apache Wicket.
	by Inaiat Henrique - inaiat@gmail.com

Notice:
	mootools-meio-mask version 2.0.1
	mootools-more version 1.3.0.1

How To Use:
	package br.com.digilabs.meiomask.examples;

	import br.com.digilabs.wicket.component.meiomask.MeioMaskField;
	import br.com.digilabs.wicket.component.meiomask.MeioMaskType;
	import org.apache.wicket.PageParameters;
	import org.apache.wicket.markup.html.WebPage;

	/**
	 * Homepage
	 */
	public class HomePage extends WebPage {

	    private static final long serialVersionUID = 1L;

	    public HomePage(final PageParameters parameters) {

		add(new MeioMaskField("change-mask-input",MeioMaskType.Fixed,"{mask: ':99:99:', autoTab: true}"));

		add(new MeioMaskField("fixed-phone",MeioMaskType.FixedPhone));
		
		add(new MeioMaskField("fixed-phone-us",MeioMaskType.FixedPhoneUs));

		add(new MeioMaskField("fixed-cpf",MeioMaskType.FixedCpf));

	    }
	}


